<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ic_launcher_background"
    android:hapticFeedbackEnabled="true"
    tools:context=".ViewApplicationDetails"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="-119dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="320dp"
        android:layout_height="69dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04"
        app:srcCompat="@drawable/application_details_heading" />

    <EditText
        android:id="@+id/application_status_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:clickable="false"
        android:ems="10"
        android:inputType="text"
        android:padding="5dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.81"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/application_reference_view" />

    <EditText
        android:id="@+id/application_feedback_view"
        android:layout_width="209dp"
        android:layout_height="96dp"
        android:layout_marginTop="144dp"
        android:background="#FFFFFF"
        android:clickable="false"
        android:ems="10"
        android:inputType="textMultiLine"
        android:padding="5dp"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.81"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/application_reference_view" />

    <EditText
        android:id="@+id/application_modified_date_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:background="#FFFFFF"
        android:clickable="false"
        android:ems="10"
        android:inputType="text"
        android:padding="5dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.81"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/application_reference_view" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="119dp"
        android:layout_height="34dp"
        android:layout_marginTop="144dp"
        android:fontFamily="@font/baloo_bhaina"
        android:text="Feedback"
        android:textAlignment="textEnd"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/application_reference_view"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="108dp"
        android:layout_height="56dp"
        android:layout_marginTop="-2dp"
        android:fontFamily="@font/baloo_bhaina"
        android:lineHeight="15sp"
        android:text="ADOPTER DETAILS"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adopter_details_icon_view" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="108dp"
        android:layout_height="56dp"
        android:layout_marginTop="-2dp"
        android:fontFamily="@font/baloo_bhaina"
        android:lineHeight="15sp"
        android:text="DOG\nDETAILS"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dog_details_icon_view" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="119dp"
        android:layout_height="34dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/baloo_bhaina"
        android:text="Status"
        android:textAlignment="textEnd"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/application_reference_view"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="119dp"
        android:layout_height="34dp"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/baloo_bhaina"
        android:text="Date Modified"
        android:textAlignment="textEnd"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/application_reference_view"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="119dp"
        android:layout_height="34dp"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/baloo_bhaina"
        android:text="Date Submitted"
        android:textAlignment="textEnd"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/application_reference_view"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/application_submitted_date_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:background="#FFFFFF"
        android:clickable="false"
        android:ems="10"
        android:inputType="text"
        android:padding="5dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.81"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/application_reference_view" />

    <EditText
        android:id="@+id/application_reference_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:background="#FFFFFF"
        android:clickable="false"
        android:ems="10"
        android:inputType="text"
        android:padding="5dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.805"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="119dp"
        android:layout_height="34dp"
        android:layout_marginTop="112dp"
        android:fontFamily="@font/baloo_bhaina"
        android:text="Reference Code"
        android:textAlignment="textEnd"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/application_reference_view"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="320dp"
        android:layout_height="69dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/application_feedback_view"
        app:srcCompat="@drawable/other_information_heading" />

    <ImageView
        android:id="@+id/dog_details_icon_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toEndOf="@+id/adopter_details_icon_view"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/dog_icon" />

    <ImageView
        android:id="@+id/adopter_details_icon_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/person_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>